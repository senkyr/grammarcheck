{
  "name": "grammarcheck",
  "version": "1.0.0",
  "description": "Pomůcka k procvičování české gramatiky",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install && cd frontend && npm install && npm run build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "natural": "^6.5.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/senkyr/grammarcheck.git"
  },
  "keywords": [
    "grammar",
    "czech",
    "school"
  ],
  "author": "Jakub Šenkýř",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/senkyr/grammarcheck/issues"
  },
  "homepage": "https://github.com/senkyr/grammarcheck#readme"
}